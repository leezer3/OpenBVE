  <Import Project="$(MSBuildBinPath)\Microsoft.CSharp.targets" />
  <!-- AUTOMATICALLY TRANSLATE TEXT TEMPLATES -->
  <!-- Optionally make the import portable across VS versions -->
  <PropertyGroup>
    <!-- Get the Visual Studio version – defaults to 10: -->
    <VisualStudioVersion Condition="'$(VisualStudioVersion)' == ''">10.0</VisualStudioVersion>
    <!-- Keep the next element all on one line: -->
    <VSToolsPath Condition="'$(VSToolsPath)' == ''">$(MSBuildExtensionsPath32)\Microsoft\VisualStudio\v$(VisualStudioVersion)</VSToolsPath>
  </PropertyGroup>
  <!-- This is the important line: -->
  <Import Condition=" '$(OS)' != 'Unix' and Exists('$(VSToolsPath)\TextTemplating\Microsoft.TextTemplating.targets')" Project="$(VSToolsPath)\TextTemplating\Microsoft.TextTemplating.targets" />
  <!-- Details about translation -->
  <PropertyGroup>
    <TransformOnBuild>true</TransformOnBuild>
  </PropertyGroup>
  <PropertyGroup>
    <OverwriteReadOnlyOutputFiles>true</OverwriteReadOnlyOutputFiles>
  </PropertyGroup>
  <PropertyGroup>
    <TransformOutOfDateOnly>true</TransformOutOfDateOnly>
  </PropertyGroup>
  <PropertyGroup>
    <!-- Generate AssemblyInfo.cs -->
    <!-- NOTE: Microsoft.TextTemplating.targets cannot process files that contain LF. -->
    <PreBuildEvent Condition=" '$(OS)' != 'Unix' ">$(ProjectDir)..\DevTools\TextTransform\TextTransform.exe $(ProjectDir)\Properties\AssemblyInfo.tt</PreBuildEvent>
    <PreBuildEvent Condition=" '$(OS)' == 'Unix' ">mono $(ProjectDir)../DevTools/TextTransform/TextTransform.exe $(ProjectDir)Properties/AssemblyInfo.tt</PreBuildEvent>
  </PropertyGroup>
  <Target Name="AfterBuild">
    <!-- Set LBA flag -->
    <Exec Condition="'$(OS)' != 'Unix'" Command="$(TargetDir)DevTools\LBAHeader.exe $(TargetDir)OpenBve.exe" />
    <Exec Condition="'$(OS)' == 'Unix'" Command="mono $(TargetDir)DevTools/LBAHeader.exe $(TargetDir)OpenBve.exe" />
    <!-- Copy in dependancies -->
    <!-- NOTE: Whilst AtsPluginProxy.dll is Windows specific, we still want to copy it in the auto-generated nightly builds -->
    <ItemGroup>
      <AssetFiles Include="$(SolutionDir)assets\**\*.*" />
    </ItemGroup>
    <Copy SkipUnchangedFiles="true" SourceFiles="@(AssetFiles)" DestinationFiles="@(AssetFiles->'$(TargetDir)Data\%(RecursiveDir)%(Filename)%(Extension)')" />
  </Target>
  <Target Name="AfterClean">
    <Delete Files="Properties\AssemblyInfo.cs" />
  </Target>
